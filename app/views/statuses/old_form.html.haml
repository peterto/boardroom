= simple_form_for(@status, :html => {:class => "form-horizontal"}) do |f|
  = render 'shared/error_msg', model: @status
  = f.input :name, :input_html => {:class => "span6"}
  = f.input :image, :input_html => {:class => "span6"}
  = f.input :description, :input_html => {:class => "span6"}
  = f.button :submit, :class => "btn-primary"
